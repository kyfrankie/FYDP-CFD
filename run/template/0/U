FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (22.94397 1.60440 0.0 );

boundaryField
{
    "(outlet|inlet|far_side)"
    {
        type            freestream;
        freestreamValue $internalField;
    }

    "(wing_surface|wing_tip)"
    {
        type            noSlip;
    }

    near_side
    {
        type            symmetryPlane;
    }

}


