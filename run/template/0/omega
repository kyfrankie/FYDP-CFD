FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    object      omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 127.25;

boundaryField
{
    "(inlet|top|bottom|outlet)"
    {
        type            freestream;
        freestreamValue $internalField;
    }

    side
    {
        type            slip;
    }

    wall
    {
        type            omegaWallFunction;
        value           $internalField;
    }

    symmetry
    {
        type            symmetry;
    }

}


// ************************************************************************* //
