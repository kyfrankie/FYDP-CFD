FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     pimpleFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.4;

deltaT          1e-6;

writeControl    adjustableRunTime;

writeInterval   0.02;

purgeWrite      3;

writeFormat     ascii;

writePrecision  8;

writeCompression off;

timeFormat      general;

timePrecision   6;

runTimeModifiable true;

adjustTimeStep  yes;

maxCo           5.0;

functions
{
  residuals
  {
      type              residuals;
      libs              ("libutilityFunctionObjects.so");
      writeControl      timeStep;
      writeInterval     1;
      fields
      (
          p
          U
          k
          omega
      );
  }
}

